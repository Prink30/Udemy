package POMEx;

import org.testng.annotations.Test;

public class Tutorial2 {
	
	// Grouping test cases
	
	@Test(priority=-3)
	void test1(){
	System.out.println("jasmine");	
	}

	@Test(priority=1)
	void test2() {
		System.out.println("apple");	
	}
	
	@Test(priority=2)
	void test3() {
		System.out.println("rose");
	}
	
	@Test(priority=3)
	void test4() {
		System.out.println("mango");	
	}
}
