package POMEx;

import org.testng.annotations.Test;

public class Tutorial2 {
	
	// Grouping test cases
	
	@Test(priority=1,groups="flower")
	void test1(){
	System.out.println("jasmine");	
	}

	@Test(priority=2,groups="fruit")
	void test2() {
		System.out.println("apple");	
	}
	
	@Test(priority=3,groups="flower")
	void test3() {
		System.out.println("rose");
	}
	
	@Test(priority=4,groups="fruit")
	void test4() {
		System.out.println("mango");	
	}
}
